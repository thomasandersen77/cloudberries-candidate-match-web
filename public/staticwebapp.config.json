{
  "routes": [
    {
      "route": "/api/{*path}",
      "methods": [
        "GET",
        "HEAD",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "OPTIONS"
      ],
      "allowedRoles": [
        "anonymous",
        "admin",
        "user"
      ],
      "rewrite": "https://cloudberries-candidate-match-ca.whitesand-767916af.westeurope.azurecontainerapps.io/{path}"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/assets/*"
    ]
  }
}

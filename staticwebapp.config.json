{
  "routes": [
    {
      "route": "/api/actuator/{*rest}",
      "rewrite": "/actuator/{rest}",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/{*rest}",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/assets/*",
      "/*.{png,jpg,gif,svg,ico,css,js}"
    ]
  }
}
